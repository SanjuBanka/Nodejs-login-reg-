<%- include('menu.ejs') %>

<div class="container">
  <h1>user details</h1>
  <table class="table table-striped table-hover table-bordered shadow">
    <thead class="table-dark">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
      </tr>
    </thead>

    <tbody>
      <% data.forEach(uers => { %>
      <tr>
        <td><%= uers.name %></td>
        <td><%= uers.email %></td>
        <td><%= uers.password %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
